{
  "name": "WinUp",
  "version": "1.0.1",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "@reduxjs/toolkit": "^2.8.2",
    "expo": "~53.0.20",
    "expo-font": "^13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-notifications": "^0.31.4",
    "expo-secure-store": "^14.2.3",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-redux": "^9.2.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native/eslint-config": "^0.81.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.4",
    "prettier": "^3.6.2",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "^@components(.*)": "<rootDir>/src/components$1",
      "^@hooks(.*)": "<rootDir>/src/hooks$1",
      "^@navigation(.*)": "<rootDir>/src/navigation$1",
      "^@services(.*)": "<rootDir>/src/services$1",
      "^@store(.*)": "<rootDir>/src/store$1",
      "^@types(.*)": "<rootDir>/src/types$1",
      "^@viewmodels(.*)": "<rootDir>/src/viewmodels$1",
      "^@validators(.*)": "<rootDir>/src/validators$1",
      "^@config(.*)": "<rootDir>/src/config$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-native-reanimated|expo(nent)?|@expo(nent)?/.*|@unimodules/.*|react-clone-referenced-element|@react-navigation/.*|react-redux|@reduxjs/.*|redux)/"
    ]
  },
  "private": true
}
